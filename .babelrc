{
  "presets": [
    "react",
    ["env", {
      "modules": false
    }]
  ],
  "plugins": [
    ["transform-class-properties", { "spec": true }],
    ["transform-runtime", {
      "helpers": false,
      "polyfill": false,
      "regenerator": true,
      "moduleName": "babel-runtime"
    }]
  ]
}
